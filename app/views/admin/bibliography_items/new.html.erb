<h2>Add a new news item</h2>

<%= simple_form_for @bibliography_item, :url => admin_bibliography_items_path do |f| %>
	
	<%= render :partial => 'admin/bibliography_items/form', :locals => { :f => f } %>

  <%= render :partial => 'admin/shared/form_buttons', :locals => { f: f, item: @bibliography_item } %>
	
<% end %>
