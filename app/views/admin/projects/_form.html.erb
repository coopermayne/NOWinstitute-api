<h2>Other Stuff</h2>
<div class="row">
	<div class="col-lg-6">
		<%= f.input:title %>
	</div>

	<div class="col-lg-6">
    <% if f.object && f.object.primary_image %>
    <%= image_tag f.object.primary_image.name_url, class: "img-responsive" %>
  <% end %>
	</div>

</div>

<div class="row">
  <div class="col-lg-12">
    <%= f.input :overview, rows: "8" %>
  </div>
</div>
<div class="row">
  <div class="col-lg-12">
    <%= f.input :description, rows: "20" %>
  </div>
  <div class="col-lg-12">
    <%= f.input :program %>
  </div>
</div>

<h2>details</h2>

<div class="row">
  <div class="col-md-6">
    <%= f.input :client %>
  </div>
  <div class="col-md-6">
    <%= f.input :size %>
  </div>

  <div class="col-md-6">
    <%= f.input :height %>
  </div>

  <div class="col-md-6">
    <%= f.input :site_area %>
  </div>
  <div class="col-md-6">
    <%= f.input :lat %>
  </div>
  <div class="col-md-6">
    <%= f.input :lon %>
  </div>
  <div class="col-md-6">
    <%= f.input :street_address %>
  </div>
  <div class="col-md-6">
    <%= f.input :zip %>
  </div>
</div>
