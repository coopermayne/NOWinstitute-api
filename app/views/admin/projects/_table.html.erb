<table class="table table-striped">
	<thead>
		<tr>
      <th><%= sortable "title" %></th>
      <th><%= sortable "design_edate" %></th>
      <th><%= sortable "constr_edate" %></th>
      <th>Image</th>
      <th>Visible</th>
			<th>&nbsp;</th>
		</tr>
	</thead>
	
	<% projects.each do |project| %>
		<tr>
      <td><%= link_to project.title, admin_project_path(project) %>
      <td><%= project.design_edate %>
      <td><%= project.constr_edate %>
      <td><%= project.primary_image ? fa_icon("check", class: "pull-right", style: "color:green") : fa_icon("close", class: "pull-right", style: "color: red") %>

			<td class="delete">
			</td>
		</tr>
	<% end %>
</table>
