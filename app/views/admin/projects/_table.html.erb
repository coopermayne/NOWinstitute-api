<table class="table table-striped">
	<thead>
		<tr>
      <th><%= sortable "title" %></th>
      <th><%= sortable "design_edate" %></th>
      <th><%= sortable "constr_edate" %></th>
      <th>primary image</th>
			<th>&nbsp;</th>
		</tr>
	</thead>
	
	<% projects.each do |project| %>
		<tr>
      <td><%= link_to project.title, admin_project_path(project) %>
      <td><%= project.design_edate %>
      <td><%= project.constr_edate %>
      <td><%= project.primary_image ? "true" : "false" %>

			<td class="delete">
        <%= link_to('del', admin_project_path(project), :method => :delete, :data => { :confirm => "Are you sure you want to delete this project?" }) %>
			</td>
		</tr>
	<% end %>
</table>
