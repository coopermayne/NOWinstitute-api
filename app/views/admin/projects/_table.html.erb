<table class="table table-striped index-table">
	<thead>
		<tr>
      <th>title</th>
      <th>design</th>
      <th>con</th>
      <th>created</th>
      <th></th>
		</tr>
	</thead>
	
	<% projects.each do |project| %>
		<tr>
      <td><%= link_to project.title, admin_project_path(project) %>
      <td><%= project.design_edate %>
      <td><%= project.constr_edate %>
      <td><%= project.created_at.strftime("%m/%d/%Y") %>
			<td>&nbsp;</td>
		</tr>
	<% end %>
</table>
