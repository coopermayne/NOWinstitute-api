<h2>View project</h2>

<%= simple_form_for @project, :url => admin_project_path(@project) do |f| %>

  <%= render :partial => 'admin/projects/form', :locals => { :f => f } %>
	
	<p class="submits">
	  	<%= link_to 'Cancel', admin_projects_path, :class => 'btn btn-mini btn-danger' %>
		<%= f.submit "Save changes", :class => "btn btn-mini btn-primary" %>
	</p>

<% end %>
