<h2><%= @title %></h2>

<table class='table'>
  <thead>
    <tr>
      <th colspan>email</th>
      <th colspan>name</th>

      <th colspan>&nbsp;</th>
      <th colspan>&nbsp;</th>
      <th colspan>&nbsp;</th>
    </tr>
  </thead>

  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.email %></td>
        <td><%= user.name %></td>
        <td><%= link_to 'Edit', edit_admin_user_path(user) %></td>
        <td><%= link_to 'Destroy', admin_user_path(user), method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New User', new_admin_user_path %>
