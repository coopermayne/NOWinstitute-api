<!-- Scaffolding generated by Casein v5.1.1.5 -->


<h2>sorting</h2>
<div class="row">

	<div class="col-lg-6">

    <%= casein_text_field f, f.object, :title %>
    <%= casein_collection_select f, f.object, :news_item, :news_type_id, NewsType.all, :id, :title %>

	</div>
  <% if f.object.primary_image %>
    <div class="col-lg-6">
      <%= image_tag f.object.primary_image.name_url, class: "img-responsive" %>
    </div>
  <% end %>
</div>

<div class="row">
	<div class="col-lg-6">
		<%= casein_text_area f, f.object, :description, rows: "20" %>
	</div>
	<div class="col-lg-6">
		<%= casein_text_area f, f.object, :overview, rows: "20" %>
	</div>
</div>

<div class="row">
	<div class="col-lg-6">
		<%= casein_text_field f, f.object, :place_name %>
	</div>

	<div class="col-lg-6">
		<%= casein_text_field f, f.object, :street_address %>
	</div>
	<div class="col-lg-6">
		<%= casein_date_select f, f.object, :start_date %>
	</div>
</div>
