<!-- Scaffolding generated by Casein v5.1.1.5 -->

<table class="table table-striped table-hover table-condensed">
	<thead>
		<tr>
			<th><%= casein_sort_link "Title", :title %></th>
			<th><%= casein_sort_link "Overview", :overview %></th>
			<th><%= casein_sort_link "Description", :description %></th>
			<th><%= casein_sort_link "Program", :program %></th>
			<th><%= casein_sort_link "Client", :client %></th>
			<th><%= casein_sort_link "Size", :size %></th>
			<th><%= casein_sort_link "Site area", :site_area %></th>
			<th><%= casein_sort_link "Lat", :lat %></th>
			<th><%= casein_sort_link "Lon", :lon %></th>
			<th><%= casein_sort_link "Street address", :street_address %></th>
			<th><%= casein_sort_link "Zip", :zip %></th>
			<th><%= casein_sort_link "Design sdate", :design_sdate %></th>
			<th><%= casein_sort_link "Height", :height %></th>
			<th>&nbsp;</th>
		</tr>
	</thead>
	
	<% projects.each do |project| %>
		<tr>
			<td><%= casein_table_cell_link project.title, casein_project_path(project) %></td>
			<td><%= casein_table_cell_link project.overview, casein_project_path(project) %></td>
			<td><%= casein_table_cell_link project.description, casein_project_path(project) %></td>
			<td><%= casein_table_cell_link project.program, casein_project_path(project) %></td>
			<td><%= casein_table_cell_link project.client, casein_project_path(project) %></td>
			<td><%= casein_table_cell_link project.size, casein_project_path(project) %></td>
			<td><%= casein_table_cell_link project.site_area, casein_project_path(project) %></td>
			<td><%= casein_table_cell_link project.lat, casein_project_path(project) %></td>
			<td><%= casein_table_cell_link project.lon, casein_project_path(project) %></td>
			<td><%= casein_table_cell_link project.street_address, casein_project_path(project) %></td>
			<td><%= casein_table_cell_link project.zip, casein_project_path(project) %></td>
			<td><%= casein_table_cell_link project.design_sdate, casein_project_path(project) %></td>
			<td><%= casein_table_cell_link project.height, casein_project_path(project) %></td>
			<td class="delete">
				<%= link_to(casein_show_row_icon("trash"), casein_project_path(project), :method => :delete, :data => { :confirm => "Are you sure you want to delete this project?" }) %>
			</td>
		</tr>
	<% end %>
</table>